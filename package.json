    "command-exists": "^1.2.8",
    "console-stamp": "^0.2.7",
    "dotenv": "^8.1.0",
    "elementtree": "^0.1.7",
    "es6-dynamic-template": "^2.0.0",
    "express": "^4.16.4",
    "express-fileupload": "^0.4.0",
    "extract-zip": "^1.6.7",
    "fast-diff": "^1.2.0",
    "figlet": "^1.2.4",
    "fs-extra": "^8.1.0",
    "gulp-git": "^2.9.0",
    "html-entities": "^1.2.1",
    "mkdirp": "^0.5.1",
    "nedb": "^1.8.0",
    "node-xml-stream-parser": "^1.0.7",
    "nodemailer": "^6.3.0",
    "object-assign-deep": "^0.4.0",
    "puppeteer": "1.17.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "serve-static": "^1.13.2",
    "sn-project": "github:bmoers/sn-project#semver:^3.8.0",
    "sn-rest-client": "github:bmoers/sn-rest-client#semver:^2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "split": "^1.0.1",
    "strip-ansi": "^5.2.0",
    "tmp": "^0.1.0",
    "unescape": "^1.0.1",
    "uuid": "^3.3.3",
    "xml-js": "^1.6.11"
