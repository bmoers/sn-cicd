{
  "name": "sn-cicd",
  "version": "4.4.1",
  "description": "CICD service for Service-Now",
  "main": "lib/cicd.js",
  "scripts": {
    "start": "node --max_old_space_size=4096 server.js",
    "worker": "node worker.js",
    "server": "node server.js",
    "startMonitored": "nodemon --inspect server.js",
    "test": "mocha",
    "devServer": "nodemon --ignore cert/**/* --ignore db/**/* --verbose --delay 2 server.js",
    "devWorker": "nodemon --ignore cert/**/* --ignore db/**/* --verbose --delay 2 worker.js",
    "setup": "npm cache clean && rm -rf node_modules && npm install"
  },
  "author": "Boris Moers",
  "license": "LGPL-3.0",
  "dependencies": {
    "any-shell-escape": "^0.1.1",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "camelcase": "^6.0.0",
    "command-exists": "^1.2.9",
    "compression": "^1.7.4",
    "console-stamp": "^0.2.7",
    "dotenv": "^8.2.0",
    "elementtree": "^0.1.7",
    "es6-dynamic-template": "^2.0.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.2.0",
    "extract-zip": "^2.0.1",
    "fast-diff": "^1.2.0",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "gulp-git": "^2.10.1",
    "html-entities": "^1.3.1",
    "mkdirp": "^1.0.4",
    "mongo-express": "^0.54.0",
    "mongoose": "^5.10.1",
    "nedb": "^1.8.0",
    "nodemailer": "^6.4.11",
    "object-assign-deep": "^0.4.0",
    "puppeteer": "1.17.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "serve-static": "^1.13.2",
    "sn-project": "github:bmoers/sn-project#semver:^3.13.0",
    "sn-rest-client": "github:bmoers/sn-rest-client#semver:^2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "split": "^1.0.1",
    "split2": "^3.2.2",
    "strip-ansi": "^6.0.0",
    "tmp": "^0.1.0",
    "unescape": "^1.0.1",
    "uuid": "^7.0.3",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "tree-kill": "^1.2.2"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmoers/sn-cicd.git"
  },
  "keywords": [
    "CICD",
    "DevOps",
    "Service-Now"
  ],
  "bugs": {
    "url": "https://github.com/bmoers/sn-cicd/issues"
  },
  "homepage": "https://github.com/bmoers/sn-cicd#readme"
}
