{
  "name": "sn-cicd",
  "version": "4.6.20",
  "description": "CICD service for Service-Now",
  "main": "lib/cicd.js",
  "scripts": {
    "start": "node --max_old_space_size=4096 server.js",
    "worker": "node worker.js",
    "server": "node server.js",
    "startMonitored": "nodemon --inspect server.js",
    "test": "mocha",
    "devServer": "nodemon --ignore cert/**/* --ignore db/**/* --verbose --delay 2 server.js",
    "devWorker": "nodemon --ignore cert/**/* --ignore db/**/* --verbose --delay 2 worker.js",
    "setup": "npm cache clean && rm -rf node_modules && npm install"
  },
  "author": "Boris Moers",
  "license": "LGPL-3.0",
  "dependencies": {
    "bluebird": "^3.7.2",
    "camelcase": "^6.2.0",
    "compression": "^1.7.4",
    "concat-stream": "^1.6.2",
    "console-stamp": "^0.2.7",
    "dotenv": "^8.2.0",
    "elementtree": "^0.1.7",
    "es6-dynamic-template": "^2.0.0",
    "express": "^4.16.4",
    "express-fileupload": "^1.2.1",
    "extract-zip": "^2.0.1",
    "figlet": "^1.5.0",
    "fs-extra": "^9.1.0",
    "gulp-git": "^2.10.1",
    "html-entities": "^2.1.0",
    "mkdirp": "^1.0.4",
    "mongo-express": "^1.0.0-alpha.2",
    "mongoose": "^5.12.3",
    "multer": "^1.4.2",
    "nedb": "^1.8.0",
    "nodemailer": "^6.5.0",
    "object-assign-deep": "^0.4.0",
    "puppeteer": "1.17.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "sn-project": "^3.15.0",
    "sn-rest-client": "^2.1.9",
    "socket.io": "^3.0.4",
    "socket.io-client": "^3.0.4",
    "split": "^1.0.1",
    "split2": "^3.2.2",
    "strip-ansi": "^6.0.0",
    "uuid": "^8.3.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "chai": "^4.3.0",
    "chalk": "^1.1.3",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "mocha": "^8.3.0",
    "nodemon": "^2.0.7",
    "tree-kill": "^1.2.2"
  },
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bmoers/sn-cicd.git"
  },
  "keywords": [
    "CICD",
    "DevOps",
    "Service-Now"
  ],
  "bugs": {
    "url": "https://github.com/bmoers/sn-cicd/issues"
  },
  "homepage": "https://github.com/bmoers/sn-cicd#readme",
  "engines": {
    "node": ">=11.15.0",
    "npm": ">=6.0.0"
  }
}
